{
  "name": "rpc-websockets",
  "version": "4.1.0",
  "description": "JSON-RPC 2.0 implementation over WebSockets for Node.js",
  "main": "./dist/index.js",
  "scripts": {
    "build": "mkdir -p ./dist && eslint --fix -c ./.eslintrc './src/**/*.js' './test/**/*.js' && babel ./src -d ./dist",
    "pretest": "npm run-script build",
    "test": "mocha --exit test/*spec.js",
    "coverage": "istanbul cover _mocha --report lcovonly -- -R spec --exit",
    "coveralls": "npm run-script coverage && coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qaap/rpc-websockets.git"
  },
  "dependencies": {
    "assert-args": "^1.2.1",
    "babel-runtime": "^6.26.0",
    "circular-json": "^0.5.4",
    "eventemitter3": "^3.1.0",
    "uuid": "^3.2.1",
    "ws": "^5.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2016": "^6.24.1",
    "browserify": "^16.2.2",
    "chai": "^4.0.2",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.2.0"
  },
  "browser": "./dist/index.browser.js",
  "keywords": [
    "json",
    "rpc",
    "websocket",
    "ws",
    "client",
    "server"
  ],
  "author": "Qaap",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qaap/rpc-websockets/issues"
  },
  "homepage": "https://github.com/qaap/rpc-websockets#readme"
}
